extends ../layouts/managerLayout
block content
    div(style="text-align: center;height: 50px;padding-top: 50px;")
        input#input-1(type="checkbox")
        input#input-2(type="checkbox",checked="checked")
        input#input-3(type="checkbox",disabled="disabled")
        input#input-4(type="checkbox",checked="checked",disabled="disabled")
    script(type="text/javascript").
        require(['jquery','iCheck'],function($){
            $('input[id^=input]').iCheck();
        });